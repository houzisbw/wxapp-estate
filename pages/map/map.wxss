/* pages/map/map.wxss */
page{
    width:100%;
    height:100%;
}
.map-wrapper{
    width:100%;
    height:100%;
}
.map{
    width:100%;
    height:100%;
}
.top-shadow-wrapper{
    position: absolute;
    left:0;
    top:0;
    width:100%;
    height:10rpx;
}
.top-shadow,.bottom-shadow{
    width:100%;
    height:100%;
}
.bottom-shadow-wrapper{
    position: absolute;
    left:0;
    bottom:0;
    width:100%;
    height:10rpx;
}